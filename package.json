{
  "name": "tinyclient",
  "version": "0.6.1",
  "description": "a tiny client",
  "scripts": {
    "build": "webpack --colors",
    "build:watch": "webpack --watch --progress --colors",
    "changelog": "github-changes -o wingedearth -r tinyclient -b dev -a --only-merges && git add CHANGELOG.md && git commit -m 'update changelog'",
    "clean": "rimraf build",
    "dev": "ENVIRONMENT=default npm run build:watch & ENVIRONMENT=default npm start",
    "prebuild": "npm run clean",
    "predev": "npm run sass:watch & ENVIRONMENT=default npm run build",
    "postversion": "npm run changelog && git push && git push --tags",
    "prod": "npm run sass && ENVIRONMENT=production npm run build && ENVIRONMENT=production npm start",
    "sass": "node-sass --output-style nested --indent-type tab --indent-width 1 ./src/client/scss/main.scss ./src/client/css/main.css",
    "sass:watch": "npm run sass -- -w ./src/client/scss",
    "start": "node ./build/js/server.js",
    "test": "mocha --compilers js:babel-core/register --recursive --reporter=spec tests",
    "test:coverage": "cross-env NODE_ENV=test nyc mocha --recursive --reporter=nyan tests",
    "test:report": "npm run test:coverage && open ./coverage/lcov-report/index.html",
    "test:file": "node ./scripts/testSingleFile.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wingedearth/tinyclient.git"
  },
  "author": "Andrew A. Anissi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wingedearth/tinyclient/issues"
  },
  "homepage": "https://github.com/wingedearth/tinyclient#readme",
  "nyc": {
    "lines": 10,
    "statements": 10,
    "functions": 10,
    "branches": 10,
    "exclude": [
      ".nyc_output",
      "build",
      "coverage",
      "**/node_modules/**",
      "src/**/*-test.js",
      "tests",
      "webpack.config.js",
      "webpack",
      "scripts",
      "src/assets"
    ],
    "require": [
      "babel-core/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "cache": true,
    "check-coverage": true,
    "all": true,
    "sourceMap": false,
    "instrument": false
  },
  "dependencies": {
    "babel-plugin-istanbul": "3.0.0",
    "babel-runtime": "6.23.0",
    "chai": "3.5.0",
    "chai-enzyme": "0.7.1",
    "colors": "1.1.2",
    "commander": "2.9.0",
    "create-react-class": "15.6.0",
    "cross-env": "5.0.1",
    "ejs": "2.5.6",
    "enzyme": "2.8.2",
    "eslint": "3.19.0",
    "esprima": "4.0.0",
    "express": "4.15.2",
    "express-react-views": "0.10.2",
    "fs": "0.0.2",
    "fsevents": "1.1.1",
    "git-rev": "0.2.1",
    "html-webpack-plugin": "2.28.0",
    "jsdomify": "^3.1.0",
    "jsonfile-promised": "0.0.1",
    "lodash": "4.17.4",
    "mocha": "3.4.2",
    "mocha-loader": "1.1.1",
    "node-sass": "4.5.3",
    "nyc": "11.0.2",
    "path": "0.12.7",
    "prop-types": "15.5.10",
    "proxyquire": "1.8.0",
    "react": "15.5.4",
    "react-addons-test-utils": "15.6.0",
    "react-dom": "15.5.4",
    "react-hot-loader": "3.0.0-beta.7",
    "react-redux": "5.0.5",
    "redux": "3.6.0",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.2.0",
    "require-uncached": "1.0.3",
    "rewire": "2.5.2",
    "rimraf": "^2.6.1",
    "sass-loader": "6.0.6",
    "sinon": "2.3.4",
    "webpack": "2.3.3",
    "webpack-node-externals": "1.5.4",
    "whenjs": "0.0.4",
    "yargs": "8.0.1"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.2",
    "babel-loader": "6.4.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-env": "1.4.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.24.1",
    "css-loader": "0.28.4",
    "eslint-html-reporter": "0.5.2",
    "eslint-plugin-babel": "4.1.1",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-react": "7.0.1",
    "extract-text-webpack-plugin": "2.1.2",
    "github-changes": "1.1.0",
    "style-loader": "0.18.2",
    "webpack-dev-middleware": "1.10.2",
    "webpack-hot-middleware": "2.18.0"
  }
}
